                    <br />
                    <span>Search</span>
                    <br />
                    <form>
                        <input type="text" name=""/>
                        <input type="submit" name=""/>
                    </form>
                    <br />
                    <hr />
                </div>
                  <div class="container">
                      <h1 style="text-align:center">Manuscripts</h1>
                      <div class="row container" style="margin:0px auto">
                            <br />
                            <img src="~/img/anobis.jpg" width="900" height="270" style="border:1px solid white;border-radius:10px"/> 
                      </div>
                      <br />
                      <hr />
                      <div class="container">
                        @foreach (var item in Model){
                          <div class="card" style="width: 18rem;">
                                  <img src="@Html.DisplayFor(modelItem => item.scripturl)" class="card-img-top" alt="...">
                                  <div class="card-body">
                                    <h5 class="card-title">@Html.DisplayFor(modelItem => item.scriptname)</h5>
                                    <p class="card-text">Some quick example text to build on the card title and make up the bulk of the card's content.</p>
                                  </div>
                                  <ul class="list-group list-group-flush">
                                    <li class="list-group-item">@Html.DisplayFor(modelItem => item.scriptdiscription)</li>
                                    <li class="list-group-item">@Html.DisplayFor(modelItem => item.scriptlang)</li>
                                    <li class="list-group-item"> @Html.DisplayFor(modelItem => item.scripttopic)</li>
                                   <li class="list-group-item">@Html.DisplayFor(modelItem => item.scriptdate)</li>
                                </ul>
                                  <div class="card-body">
                                    <a asp-action="Details" asp-route-id="@item.id" class="card-link">Details</a>
                                   
                                  </div>
                                </div>
                                }
                      </div>

                  

                </div>
            </div>
            <div class="col-1">

            </div>
         </div>
    </div>


