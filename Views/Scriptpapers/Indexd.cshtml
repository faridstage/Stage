                @Html.DisplayFor(modelItem => item.scriptcode)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.scriptpalcestore)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.scriptimage)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.scripturl)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.scriptnote)
            </td>
